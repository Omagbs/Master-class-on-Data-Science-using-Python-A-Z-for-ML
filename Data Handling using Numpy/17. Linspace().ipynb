  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "6b0f4ebc-c291-405c-9d83-a350a411f948",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
